<?xml version="1.0" encoding="UTF-8"?>
<!-- ActionScript代码模板 -->
<templates language="ActionScript3">


<!-- 文件模板-->
<template id="file" name="文件模板">
package ${enclosing_type};&#13;
{&#13;
	${import}&#13;
	
	/**&#13;
 	 * @author ${enclosing_type}&#13;
 	 */&#13;
	${modifiers} ${type} ${enclosing_type} ${extends} ${implements}&#13;
	{&#13;
		${constructor}&#13;
		
		${body}&#13;
	}	&#13;
}</template>

<!-- import com.cn.beijing.chaoyang.guanzhuang.jia5; -->
<template id="import" name="导入模板 ">import ${enclosing_type};</template>

<!-- extends Class-->
<template id="extends" name="继承模板">extends ${enclosing_type};</template>

<!-- implements IInterface1, IInterface2, IInterface3.... -->
<template id="implements" name="实现接口模板">implements ${enclosing_type};</template>

<!-- name:type=value -->
<template id="params" name="参数代码块">${enclosing_type}:${enclosing_type}${enclosing_type}</template>

<!-- @@@@@@@@@@@方法模板start@@@@@@@@@@@ -->

<template id="function" name="function">${namespace} ${modifiers}function ${method_name}(${params}):${return_type}&#13;
{&#13;
	// TODO Auto Generated method stub&#13;
	${body_statement}${cursor}&#13;
}</template>

<template id="getter" name="getter">${metadata}&#13;
${asdoc}&#13;
${namespace} ${modifiers}function get ${method_name}()${return_type}&#13;
{&#13;
	return ${property};&#13;
}</template>

<template id="setter" name="setter">${asdoc}&#13;
${namespace} ${modifiers}function set ${method_name}(value${argument_type}):void&#13;
{&#13;
	${property} = value;&#13;
}</template>

<!-- @@@@@@@@@@@方法模板end@@@@@@@@@@@ -->

<!-- @@@@@@@@@@@循环遍历start@@@@@@@@@@@ -->

<!--fore循环 -->
<template id="fore" name="fore">for each (var ${index}:int in ${array}) &#13;
{&#13;
	${line_selection}${cursor}&#13;
}
</template>

<!--fori循环 -->
<template id="fori" name="fori">for (var ${index}:int = 0; ${index} &lt; ${array}.length; ${index}++) &#13;
{&#13;
	${line_selection}${cursor}&#13;
}
</template>

<!--switch代码块 -->
<template id="switch" name="switch">switch(${expression})&#13;
{&#13;
	case ${result}:&#13;
	{&#13;
		${cursor}&#13;
		break;&#13;
	}&#13;
	&#13;
	default:&#13;
	{&#13;
		break;&#13;
	}&#13;
}</template>

<!--do函数代码块 -->
<template id="do" name="do">do &#13;
{&#13;
	${line_selection}${cursor}&#13;
} while(${condition:local_var(Boolean)});</template>

<!-- @@@@@@@@@@@循环遍历end@@@@@@@@@@@ -->

<!-- @@@@@@@@@@@条件判断start@@@@@@@@@@@ -->

<!--if判断代码块 -->
<template id="if" name="if">if (${condition:local_var(Boolean)}) &#13;
{&#13;
	${cursor}&#13;
}</template>

<!-- @@@@@@@@@@@条件判断end@@@@@@@@@@@ -->

<template id="tryCatch" name="tryCatch">try&#13;
{&#13;
	${line_selection}${cursor}&#13;
} &#13;
catch(${error}:${Error}) &#13;
{&#13;
	&#13;
}</template>

</templates>
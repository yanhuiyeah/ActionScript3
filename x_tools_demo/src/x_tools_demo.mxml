<?xml version="1.0" encoding="utf-8"?>
<s:WindowedApplication xmlns:fx="http://ns.adobe.com/mxml/2009" 
					   xmlns:s="library://ns.adobe.com/flex/spark" width="1280" height="900"
					   xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			import com.encoder.FileProcessor;
			protected function button1_clickHandler(event:MouseEvent):void
			{
				FileProcessor.instance.open();
			}
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<mx:Panel id="p" x="300" y="10" width="800" height="600"/>
	<s:Button x="29" y="25" label="按钮" click="button1_clickHandler(event)"/>
</s:WindowedApplication>
